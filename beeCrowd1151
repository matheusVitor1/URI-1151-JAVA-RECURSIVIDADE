import java.io.IOException;
 
public class Main {

 public static int fatorial (int n){
 
      if (n == 0){
      
          return 1;
      
      }else{
      
       return n * fatorial(n-1);
      }
 
 }  
 
    public static void main(String[] args) throws IOException {
 
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(fatorial(n));
 
    }
 
}
